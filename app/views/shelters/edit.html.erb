<form action="/shelters/id" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter new shelter information:</p>
  <input type='text' name='shelter[name]'/><br/>
  <p>Enter address:</p>
  <textarea name='shelter[address]'></textarea><br/>
  <p>Enter city:</p>
  <textarea name='shelter[city]'></textarea><br/>
  <p>Enter state:</p>
  <textarea name='shelter[state]'></textarea><br/>
  <p>Enter zip:</p>
  <textarea name='shelter[zip]'></textarea><br/>
  <input type='submit'/>
</form>
