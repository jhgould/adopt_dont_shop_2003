<ul>
    <li><a href="/shelters"> Shelter Index</a></li>
    <li><a href="/pets"> Pets Index</a></li>
</ul>


<form action="/shelters" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter a new shelter name:</p>
  <input type='text' name='shelter[name]'/><br/>
  <p>Enter address:</p>
  <textarea name='shelter[address]'></textarea><br/>
  <p>Enter city:</p>
  <textarea name='shelter[city]'></textarea><br/>
  <p>Enter state:</p>
  <textarea name='shelter[state]'></textarea><br/>
  <p>Enter zip:</p>
  <textarea name='shelter[zip]'></textarea><br/>
  <%= submit_tag "Create Shelter" %>
</form>
